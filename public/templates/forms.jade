    form(class="o-datepicker")
      h4(class="c-title") Date
        span
          i(class="mdi mdi-calendar"  style="font-size: 20px; margin-left:5px; display: block")
            span
              input(class="c-calendar" id="datepick1" type="date")
          
      h4(class="c-title") Exclude Date
        span
          i(class="mdi mdi-calendar-remove" style="font-size: 20px; margin-left:5px")
            span
              input(class="c-calendar" id="datepick2" type="date")
