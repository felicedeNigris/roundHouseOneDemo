div(class="m-form-container")
  div(class="m-inputform")
    form(class="o-nameinputfield")
      h4(class="c-data-title") Data Inputs
      input(class="c-input" type="text" placeholder="Enter Name")
      i(class="mdi  mdi-tag-outline" style="font-size: 20px; margin-left:5px")
        span
          input(class="c-input" type="text" placeholder="Enter Tags" style="width: 234px")

      
    form(class="o-datafilter")
      p Axis Type
        span 
          button(class="c-button") Time
      p Attribute
        span(class="c-attribute-buttons")
          button(class="c-button") Data

          button(class="c-button") Group
      br
      
    form(class="o-timezoneform")
      h4(class="c-title") Select a Time Zone
        span
